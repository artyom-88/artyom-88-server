  "homepage": "http://artyom-ganev.github.io",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-node/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-node.git"
  },
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "client:build": "cd ./client && yarn run build",
    "client:install": "cd ./client && yarn install",
    "client:start": "cd ./client && yarn run start",
    "server:install": "cd ./server && yarn install",
    "server:build": "cd ./server && yarn run build",
    "server:prod": "cd ./server && yarn run start:prod",
    "server:start": "cd ./server && yarn run start",
    "server:start:dev": "cd ./server && yarn run start:dev",
    "server:test": "cd ./server && yarn run test",
    "test": "yarn run server:test",
    "postinstall": "yarn run client:install && yarn run server:install"
  }
}
